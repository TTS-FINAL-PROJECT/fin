{
  "name": "final-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bsync": "browser-sync start --proxy \"localhost:1337\" --ss \"public\" -f \"app/index/views\" \"public/index\"",
    "build": "npm run css | npm run js",
    "coverage": "nyc ava app/**/test/*.js lib/test/*.js",
    "css": "stylus -w app/index/css/main.styl --out public/main.css",
    "js": "webpack --optimize-minimize",
    "lint": "standard",
    "start": "node app.js | npm run watch:index",
    "test": "ava app/**/test/**/*.js lib/test/**/*.js",
    "watch:index": "npm run css | npm run js | npm run bsync"
  },
  "standard": {
    "ignore": [
      "public/shared",
      "app/shared"
    ]
  },
  "dependencies": {
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "moment": "^2.10.6",
    "mongoose": "^4.1.10",
    "morgan": "^1.6.1",
    "pug": "^2.0.0-beta3",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "voorhees": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.8.2",
    "jeet": "^6.1.2",
    "nyc": "^10.1.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rupture": "^0.6.1",
    "standard": "^8.6.0",
    "stylus": "^0.54.5",
    "webpack": "^2.3.2",
    "yargs": "^4.8.1"
  }
}
